{
  "name": "anuvad",
  "description": "learning languages while browsing",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://medium.com/*",
        "https://*.substack.com/p/*"
      ]
    }
  ]
}
